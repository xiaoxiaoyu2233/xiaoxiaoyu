	}
	----UIGradient31----
	--Script32--
	local Script32 = Instance.new("Script",UIGradient31)
	Script32["Name"] = "流动"
	----Script32----
	--Script33--
	local Script33 = Instance.new("Script",TextButton29)
	----Script33----
	--Frame34--
	local Frame34 = Instance.new("Frame",Frame21)
	Frame34["Visible"] = false
	Frame34["BackgroundTransparency"] = 1
	Frame34["Position"] = UDim2.new(0.339660764, 0, 0.267997772, 0)
	Frame34["Size"] = UDim2.new(0.329135686, 0, 0.413000017, 0)
	Frame34["Name"] = "main"
	Frame34["BackgroundColor3"] = Color3.new(0.137255, 0.137255, 0.137255)
	----Frame34----
	spawn(function() --Source for Script4
		local script = Script4
		local UIGradient = script.parent
		while true do
			UIGradient.Rotation += 3
			wait()
		end
	end)
	spawn(function() --Source for Script5
		local script = Script5
		local Button = script.Parent
		local Buttons = script.Parent.Parent
		
		local Frames = script.Parent.Parent.Parent.Frames
		local player = Frames.player
		
		Button.MouseButton1Click:Connect(function()
			player.Visible = true
		end)
	end)
	spawn(function() --Source for Script10
		local script = Script10
		local UIGradient = script.parent
		while true do
			UIGradient.Rotation += 3
			wait()
		end
	end)
	spawn(function() --Source for Script12
		local script = Script12
		local Button = script.Parent
		local Buttons = script.Parent.Parent
		

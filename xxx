	TextButton29["Size"] = UDim2.new(0, 62, 0, 50)
	TextButton29["Name"] = "自动防"
	TextButton29["BorderSizePixel"] = 0
	TextButton29["BackgroundColor3"] = Color3.new(1, 1, 1)
	----TextButton29----
	--UIStroke30--

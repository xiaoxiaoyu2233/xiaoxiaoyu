local uiName = "小小鱼—ucmt";
local a =
    setmetatable(
    {},
    {__index = function(self, b)
            return game.GetService(game, b)
        end, __call = function(self, b)
            return game.GetService(game, b)
        end}
)
if a.CoreGui:FindFirstChild(uiName) then
    if run then
        run:Disconnect()
        run = nil
    end
    if full then
        full:Disconnect()
        full = nil
    end
    a.CoreGui[uiName]:Destroy()
end

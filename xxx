window_a.button("标记",function()
    _G.FriendColor = Color3.fromRGB(80, 80, 255)
_G.EnemyColor = Color3.fromRGB(85, 80, 80)
_G.UseTeamColor = true

--------------------------------------------------------------------
local Holder = Instance.new("Folder", game.CoreGui)
Holder.Name = "ESP"

local Box = Instance.new("BoxHandleAdornment")
Box.Name = "nilBox"
Box.Size = Vector3.new(1, 2, 1)
Box.Color3 = Color3.new(80 / 25, 80 / 88, 80 / 80)

    J.Name = "Dropshadow"
    J.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    J.Transparency = 0.8
    J.Thickness = 2
    J.Color = Color3.fromRGB(20, 20, 20)
    J.Parent = H
